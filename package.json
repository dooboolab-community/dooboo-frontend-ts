{
  "name": "dooboo-starter",
  "version": "1.0.0",
  "description": "Build react app with typescript.",
  "postinstall": "tsc",
  "scripts": {
    "start": "webpack serve --config webpack.config.js",
    "tsc": "tsc",
    "test": "jest",
    "production": "NODE_ENV=production webpack -p --config webpack.config.prod.js",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "manifest": "babel-node node_modules/.bin/fbt-manifest --src src",
    "collect-fbts": "babel-node node_modules/.bin/fbt-collect --pretty --manifest < .src_manifest.json > .source_strings.json",
    "test-collect-fbts": "babel-node node_modules/.bin/fbt-collect --plugins @babel/plugin-syntax-flow --pretty --manifest < .src_manifest.json > .test_source_strings.json",
    "translate-fbts": "babel-node node_modules/.bin/fbt-translate --translations assets/translations/*.json --jenkins > src/utils/fbt/translatedFbts.json",
    "translate-fbts-single-file": "babel-node node_modules/.bin/fbt-translate --jenkins --stdin < translation_input.json > src/utils/fbt/translatedFbts.json",
    "test-translate-fbts-into-output-dir": "babel-node node_modules/.bin/fbt-translate --translations assets/translations/*.json --jenkins --output-dir src/translatedFbts",
    "clean-fbts": "rm .enum_manifest.json .src_manifest.json .source_strings.json src/utils/fbt/translatedFbts.json 2&> /dev/null || exit 0",
    "fbt-all": "yarn manifest && yarn collect-fbts && yarn translate-fbts"
  },
  "author": "hyochan",
  "license": "MIT",
  "dependencies": {
    "fbt": "^0.16.0",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-proposal-function-sent": "^7.12.1",
    "@babel/plugin-proposal-numeric-separator": "^7.12.5",
    "@babel/plugin-proposal-throw-expressions": "^7.12.1",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-flow": "^7.12.1",
    "@babel/preset-react": "7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/runtime": "7.12.5",
    "@dooboo/eslint-config": "^0.5.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/react": "^11.1.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-test-renderer": "^16.9.3",
    "@types/styled-components": "^5.1.4",
    "@types/webpack-env": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "abort-controller": "^3.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-fbt": "^0.16.0",
    "babel-plugin-fbt-runtime": "^0.9.12",
    "clean-webpack-plugin": "^3.0.0",
    "codecov": "^3.8.1",
    "css-loader": "^5.0.1",
    "dotenv-webpack": "^5.1.0",
    "eslint": "^7.13.0",
    "expect": "^26.6.2",
    "fbt-generate-translations": "^0.0.4",
    "file-loader": "^6.2.0",
    "history": "^5.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "react-addons-test-utils": "^15.6.2",
    "react-refresh": "^0.9.0",
    "react-test-renderer": "^17.0.1",
    "script-loader": "^0.7.2",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.10",
    "typescript": "4.0.5",
    "webpack": "^5.4.0",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-pwa-manifest": "^4.2.0",
    "workbox-webpack-plugin": "^5.1.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "resolutions": {
    "@types/react": "^16.0.40"
  }
}
